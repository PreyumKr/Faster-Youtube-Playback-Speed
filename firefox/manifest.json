{
  "manifest_version": 2,
  "name": "Faster Youtube Playback Speed",
  "version": "1.0.0",
  "description": "YouTube's < & > can only go upto 2x, let's make it go even faster.",

  "icons": { "16": "icons/16.png", "48": "icons/48.png", "128": "icons/128.png" },

  "content_scripts": [{
    "all_frames": true,
    "js": [ "ke.js" ],
    "matches": [ "https://www.youtube.com/*" ],
    "run_at": "document_start"
  }]
}
